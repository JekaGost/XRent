<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21.0.5" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cars_rent.RegisterPageController">
   <children>
      <Label fx:id="register_label" alignment="CENTER" layoutX="125.0" layoutY="14.0" prefHeight="73.0" prefWidth="350.0" text="Nerdeyse Bitti">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="firstNameField" layoutX="129.0" layoutY="87.0" promptText="İsim" />
      <TextField fx:id="usernameField" layoutX="323.0" layoutY="122.0" promptText="Kullanıcı Adı" />
      <TextField fx:id="emailField" layoutX="129.0" layoutY="122.0" promptText="E-posta" />
      <PasswordField fx:id="passwordField" layoutX="129.0" layoutY="168.0" promptText="Şifre" />
      <Button fx:id="register_devam_button" layoutX="145.0" layoutY="239.0" mnemonicParsing="false" onAction="#onRegisterDevamButton" prefHeight="109.0" prefWidth="295.0" text="Devam" />
      <Button fx:id="register_anasayfa_button" layoutX="144.0" layoutY="361.0" mnemonicParsing="false" onAction="#onRegisterAnasayfaButton" prefHeight="25.0" prefWidth="295.0" text="Anasayfa" />
      <TextField fx:id="phoneNumberField" layoutX="323.0" layoutY="168.0" promptText="Telefon Numarası giriniz..." />
      <TextField fx:id="lastNameField" layoutX="323.0" layoutY="87.0" promptText="Soyisim" />
   </children>
</AnchorPane>
