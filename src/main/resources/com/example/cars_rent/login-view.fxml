<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21.0.5" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cars_rent.LoginPageController">
   <children>
      <Button fx:id="login_button" layoutX="256.0" layoutY="253.0" mnemonicParsing="false" onAction="#onLoginClick" prefHeight="52.0" prefWidth="89.0" text="Giriş Yap" />
      <Button fx:id="login_anasayfa_button" layoutX="144.0" layoutY="361.0" mnemonicParsing="false" onAction="#onLoginAnasayfaButton" prefHeight="25.0" prefWidth="295.0" text="Anasayfa" />
      <PasswordField fx:id="passwordField" layoutX="226.0" layoutY="136.0" promptText="Şifre giriniz..." />
      <TextField fx:id="usernameField" layoutX="226.0" layoutY="80.0" promptText="Kullanıcı Adı giriniz..." />
   </children>
</AnchorPane>
