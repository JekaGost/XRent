<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cars_rent.LoginPageController">
   <children>
      <Button fx:id="login_button" layoutX="256.0" layoutY="253.0" mnemonicParsing="false" onAction="#onLoginClick" prefHeight="52.0" prefWidth="89.0" text="Login" />
      <PasswordField fx:id="passwordField" layoutX="226.0" layoutY="95.0" promptText="Password" />
      <TextField fx:id="captcha" layoutX="226.0" layoutY="200.0" promptText="Captcha" />
      <Label alignment="TOP_CENTER" contentDisplay="CENTER" layoutX="256.0" layoutY="159.0" prefHeight="17.0" prefWidth="89.0" text="2 + 2 = ?">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" layoutX="226.0" layoutY="46.0" promptText="Username" />
   </children>
</AnchorPane>
